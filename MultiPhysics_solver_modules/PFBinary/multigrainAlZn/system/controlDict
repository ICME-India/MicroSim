FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 

application     orientationFieldBinary;

startFrom       startTime;

startTime       0; // the actual time i.e. runtime.value() or no. of iterations*deltaT

stopAt          endTime;

endTime         3e-3; //actual time = total_iterations*deltaT

deltaT          1.0e-8;

writeControl    runTime;

writeInterval   2.0e-6;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


