FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 

application     phaseFieldTernary;

startFrom       startTime; //latestTime;

startTime       0; // the actual time i.e. runtime.value() or no. of iterations*deltaT

stopAt          endTime;

endTime         500e-4; //actual time = total_iterations*deltaT

deltaT          33e-8;//0.10;//0.05;//0.20;

writeControl    runTime;

writeInterval   66e-6; //0.40;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


